@model ICollection<MatchViewModel>

@using Comabit.UI.Models.Match;

<table class="table table-striped">
    <thead>
        <tr>
            <th>Firma</th>
            <th>Kategorie</th>
            <th>Anf.-Nr.</th>
            <th>Bieterfrist</th>
            <th>Lieferdatum</th>
            <th>Status</th>
            <th>Bedarf</th>
            <th>Angebot</th>
            <th>Matchdatum</th>
            <th>MatchScore</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (MatchViewModel match in Model)
        {
            @(await Html.PartialAsync("_Order", match))
        }
    </tbody>
</table>